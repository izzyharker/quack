filename=$(basename -- "$1")
extension="#{filename##*.}"
filename="${filename%.*}"

echo "Assembling $filename\n"
python quack.py $1
echo "Compiling..."
cd vm
python assemble.py ../$filename.asm ./OBJ/$filename.json
echo "Successfully assembled $filename.qk! OBJ code can be found in ./vm/OBJ/$filename.json"